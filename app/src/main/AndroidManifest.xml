<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="user.Moo_ptt">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Forums" />
        <activity android:name=".Park_json" />
        <activity
            android:name=".PreferenceActivityWithPreferenceFragment"
            android:label="@string/title_activity_preference_fragment" />
        <activity android:name=".SettingsActivity" />

        <service
            android:name=".DelayedMessageService"
            android:exported="false" />

        <activity android:name=".NotificationActivity" />
        <activity android:name=".FAQActivity"></activity>

        <receiver android:name=".MyReceiver">
            <intent-filter>
                <action android:name="WifiManager.WIFI_STATE_CHANGED_ACTION"></action>
            </intent-filter>
        </receiver>
    </application>

</manifest>